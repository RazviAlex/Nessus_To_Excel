    <sql>SELECT * FROM (
SELECT cve_num, cve.plugin, vulnerabilities.host_ip, severity, port, plugin_output, vuln_name, sol, cvss_temp3, cvss3_vector, report_name, hosts.host_name
FROM nessus.vulnerabilities
LEFT JOIN nessus.cve ON cve.plugin = vulnerabilities.plugin
INNER JOIN nessus.hosts ON hosts.host_ip = vulnerabilities.host_ip
WHERE (nessus.vulnerabilities.report_name = '${SHORT_FILEORIGINAL}')
AND (nessus_datetime like '${data_start}' OR nessus_datetime like '${data_end}')
